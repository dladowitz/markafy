  %section#contact.section.section-center.section-contact
    .container
      %h2.section-title
        %span Contact Us
      %p Want to say hello? Want to know more about us? Give us a call or drop us an email.
      .main-action
        = form_tag(contact_us_path, :role => "form") do
          .row
            .col-sm-6
              .form-group
                %label.sr-only Subject
                = text_field_tag "subject", nil, placeholder: "Subject", class: "form-control", disabled: true
              .form-group
                %label.sr-only Name
                = text_field_tag "name", nil, placeholder: "Name", class: "form-control", disabled: true
              .form-group
                %label.sr-only Email
                = email_field_tag "email", nil, placeholder: "Email", class: "form-control", disabled: true
            .col-sm-6
              .form-group
                %label.sr-only Message
                = text_area_tag "message", nil, placeholder: "Hmmm, cats have taken over our mail system for the moment. Email 'david+markafy@ladowitz.com' for call marker related emergencies.", class: "form-control", :rows => "6", :style => "height: 100px", disabled: true
          %button.btn.btn-default{:type => "submit", disabled: true} Send message

  %section.section.section-center.section-cta.pt10.pb20
    .container
